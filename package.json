{
  "name": "envir",
  "version": "1.0.1",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "get npm env variables from package.json",
  "homepage": "http://github.com/coderaiser/node-envir",
  "main": "lib/envir.js",
  "scripts": {
    "lint": "redrun lint:*",
    "lint:eslint": "eslint lib",
    "test": "tape 'test/**/*.js'",
    "watch:test": "npm run watcher -- npm test",
    "watch:coverage": "npm run watcher -- nyc npm test",
    "watch:lint": "npm run watcher -- redrun lint:eslint*",
    "watcher": "nodemon -w test -w lib --exec",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-envir.git"
  },
  "dependencies": {
    "all-object-keys": "^1.1.0",
    "jessy": "^2.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "redrun": "^6.0.0",
    "tape": "^4.8.0"
  }
}
